{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "splitPane"
            },
            "keys": "ctrl+shift+q"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "previous"
            },
            "keys": "ctrl+shift+s"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
